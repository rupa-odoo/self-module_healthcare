<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="healthcare_type_action" model="ir.actions.act_window">
        <field name="name">Healthcare Type</field>
        <field name="res_model">healthcare.type</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="healthcare_type_form" model="ir.ui.view">
       <field name="name">healthcare.type.form</field>
        <field name="model">healthcare.type</field>
        <field name="arch" type="xml">
            <form string="form">
                <sheet>
                    <h1>
                        <field name="name"></field>
                    </h1>
                    <group>
                     <field name="price" string="Price"></field>
                    </group>
                       
                </sheet>
            </form>
        </field> 
    </record>
</odoo>